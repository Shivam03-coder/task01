generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Company {
    id        String   @id @default(uuid())
    name      String
    website   String
    email     String[]
    info      String? @db.VarChar(5000)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@index([name])
}
